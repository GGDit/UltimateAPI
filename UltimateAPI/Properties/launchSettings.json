{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:28169",
      "sslPort": 44341
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": false,     //Отключение возможности запуска проекта в браузере
      "launchUrl": "weatherforecast",  //Стартовый URL-адресс
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "UltimateAPI": {
      "commandName": "Project",
      "dotnetRunMessages": "true",
      "launchBrowser": false, //Повторяет свойство, которое выше
      "launchUrl": "weatherforecast", //Повторяет свойство, которое выше
      "applicationUrl": "https://localhost:5001;http://localhost:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
